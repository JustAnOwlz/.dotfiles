command=~/.config/userscripts/$BLOCK_NAME.sh
separator_block_width=15

[volumeblock]
interval=once
signal=1

[backlightblock]
interval=once
signal=2

[keyindicator]
instance=CAPS
interval=once
signal=3

[keyindicator]
instance=NUM
interval=once
signal=3

[batteryblock]
interval=60
signal=4

[cmusblock]
interval=once
signal=5

[time]
command=date '+%Y-%m-%d %H:%M'
interval=50
